    </declare-styleable>



    <declare-styleable name="RevealFrameLayout">
        <attr name="src" format="reference" />
        <attr name="tagsBg" format="reference|color" />
        <attr name="tagsTitle" format="reference|string" />
    </declare-styleable>

</resources>