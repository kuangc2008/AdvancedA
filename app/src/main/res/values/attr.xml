<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SquareLayout">
        <attr name="my_gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="center" value="2" />
